            "<script>\n    function check(id) {\n        document.getElementById(id).checked = true;\n        document.getElementById(id).nextElementSibling.style.opacity = 1;\n        var i = 1;\n        while (document.getElementById(Math.floor(id/10) + \"\" + i) != null) {\n            if (i != id % 10) {\n                document.getElementById(Math.floor(id/10) + \"\" + i).checked = false;\n                document.getElementById(Math.floor(id/10) + \"\" + i).nextElementSibling.style.opacity = 0.7;\n        \t}\n            i++;\n        }\n        var question = 1;\n        var allAnswered = true;\n        while (document.getElementById(question + \"\" + 1) != null && allAnswered) {\n            var index = 1;\n            var checked = false;\n            while (document.getElementById(question + \"\" + index) != null) {\n                checked = checked || document.getElementById(question + \"\" + index).checked;\n                index++;\n            }\n            allAnswered = allAnswered && checked;\n            question++;\n        }\n        if (allAnswered) {\n            complete();\n        }\n    }\n</script>",
            "<script>\n    function Result(title, description, answers) {\n        this.title = title;\n        this.description = description;\n        this.count = 0;\n        this.answers = answers;\n    }\n    \n    function complete() {\n        var results = [\n            new Result(\"Interpretive Dance\", \"Show us all your dance moves with an improvised dance to the coolest 2000s pop song you can find.\", [14, 15, 23, 26, 31, 32, 43, 42, 51, 53]),\n            new Result(\"A Rap Inspired by Your Favorite Teacher\", \"Here's an example for you, going out to the 11th grade. Yo JJ, what's up hey hey, there's reading and there's reading, all those thoughts you be breeding.\", [11, 13, 23, 25, 32, 33, 44, 43, 52, 51]),\n            new Result(\"A One Man Show\", \"We at the Lion's Tale believe a play with 9 characters all played by you is the way to go.\", [12, 21, 22, 33, 35, 45, 44, 53, 56]),\n            new Result(\"Student Roast Session\", \"What better way to entertain the student body than roasting your fellow students?\", [11, 14, 23, 25, 34, 35, 36, 46, 45, 54, 55]),\n            new Result(\"A Sceance\", \"As a great school based on Christian values, bringing back some spirits seems like the way to go.\", [16, 15, 21, 26, 35, 32, 45, 45, 46, 55, 52]),\n            new Result(\"A High School Musical Song\", \"A way to easily annoy most of the boys in the audience, while making the rest of the room either extremely happy.\", [12, 13, 22, 23, 36, 34, 44, 41, 56, 51]),\n            new Result(\"A Reading of the Entire Harry Potter Series\", \"The talent show is only two hours long you say? Well, we say you can make it work.\", [13, 16, 25, 24, 35, 33, 43, 45, 55, 53]),\n            new Result(\"An Impersonation of Random World Leaders\", \"A great way to educate the student body while also making us regret what our world has come to.\", [14, 16, 21, 22, 34, 32, 42, 44, 54, 52]),\n            new Result(\"Ventriliquist\", \"Yes, for anyone but Eloi this is hard, but we believe you can fit yourself into one of the science room trash cans.\", [11, 15, 26, 24, 33, 31, 41, 43, 53, 54]),\n            new Result(\"Suicides\", \"Yep, run back and forth across the stage over and over and make everyone watch. Or you know, just do a higher level math problem on stage. Both are basically suicide.\", [12, 16, 25, 24, 32, 42, 43, 52, 51])\n        ];\n        var question = 1;\n        while (document.getElementById(question + \"\" + 1) != null) {\n            var index = 1;\n            while (document.getElementById(question + \"\" + index) != null) {\n                for (var i = 0; i < results.length; i++) {\n                    document.getElementById(question + \"\" + index).className += ' no-hover';\n                    document.getElementById(question + \"\" + index).removeAttribute('onclick');\n                    if (document.getElementById(question + \"\" + index).checked && results[i].answers.includes(parseInt(question + \"\" + index))) {\n                        console.log(\"called\");\n                        results[i].count++;\n                    }\n                }\n                index++;\n            }\n            question++;\n        }\n        results.sort(function(a, b){return b.count-a.count});\n        var maxes = [];\n        var i = 0;\n        while (results[i].count == results[0].count) {\n            maxes.push(results[i]);\n            i++;\n        }\n        var max = maxes[Math.floor(Math.random() * maxes.length)];\n        document.getElementById('title-text').innerHTML = max.title;\n        document.getElementById('desc-text').innerHTML = max.description;\n        document.getElementById('answer-container').removeAttribute('hidden');\n        window.scrollTo(0,document.body.scrollHeight);\n    }\n</script>"

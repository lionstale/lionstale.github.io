          "_name": "image2",
          "_anchor": "image2-20",
          "_params": {
            "showText": true,
            "paddingTop": "0",
            "paddingBottom": "0",
            "bgColor": "#fff"
          },
          "media": {
            "image": {
              "active": true,
              "src": "@PROJECT_PATH@/assets/images/mbr-1400x933.jpg",
              "width": 1400,
              "link": "",
              "alt": "",

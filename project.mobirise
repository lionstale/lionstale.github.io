<th><div class=\"picture-container\">\n                    <input type=\"checkbox\" class=\"image-checkbox\" id=\"16\" onclick=\"check(this.id);\">\n                    <label for=\"16\">\n                    <div class=\"text-container\"><h1 class=\"text-answer\">Reliable</h1></div></label></div></th>\n            </tr>\n    </tbody>\n    </table>\n</div>\n</form>\n\n<div class=\"header-container\">\n    <h1 class=\"header-text\">What's Your Favorite Hobby?</h1>\n</div>\n\n<form>\n<div class=\"picture-question-container\">\n    <table style=\"width: 40%\">\n        <tbody>\n            <tr>\n                <th><div class=\"picture-container\">\n                    <input type=\"checkbox\" class=\"image-checkbox\" id=\"21\" onclick=\"check(this.id);\">\n                    <label for=\"21\">\n                    <div class=\"text-container\"><h1 class=\"text-answer\">Learning Random Facts</h1></div></label></div></th>\n                <th><div class=\"picture-container\">\n                    <input type=\"checkbox\" class=\"image-checkbox\" id=\"22\" onclick=\"check(this.id);\">\n                    <label for=\"22\">\n                    <div class=\"text-container\"><h1 class=\"text-answer\">Exercise</h1></div></label></div></th>\n            </tr>\n            <tr>\n                <th><div class=\"picture-container\">\n                    <input type=\"checkbox\" class=\"image-checkbox\" id=\"23\" onclick=\"check(this.id);\">\n                    <label for=\"23\">\n                        <div class=\"text-container\"><h1 class=\"text-answer\">Cooking</h1></div></label></div></th>\n                <th><div class=\"picture-container\">\n                    <input type=\"checkbox\" class=\"image-checkbox\" id=\"24\" onclick=\"check(this.id);\">\n                    <label for=\"24\">\n                    <div class=\"text-container\"><h1 class=\"text-answer\">Something Artistic</h1></div></label></div></th>\n            </tr>\n            <tr>\n                <th><div class=\"picture-container\">\n                    <input type=\"checkbox\" class=\"image-checkbox\" id=\"25\" onclick=\"check(this.id);\">\n                    <label for=\"25\">\n                        <div class=\"text-container\"><h1 class=\"text-answer\">Doing Puzzles</h1></div></label></div></th>\n                <th><div class=\"picture-container\">\n                    <input type=\"checkbox\" class=\"image-checkbox\" id=\"26\" onclick=\"check(this.id);\">\n                    <label for=\"26\">\n                    <div class=\"text-container\"><h1 class=\"text-answer\">Reading</h1></div></label></div></th>\n            </tr>\n    </tbody>\n    </table>\n</div>\n</form>\n\n<div class=\"header-container\">\n    <h1 class=\"header-text\">Which Looks Best?</h1>\n</div>\n\n<form>\n<div class=\"picture-question-container\">\n    <table style=\"width: 40%\">\n        <tbody>\n            <tr>\n                <th><div class=\"picture-container\">\n                    <input type=\"checkbox\" class=\"image-checkbox\" id=\"31\" onclick=\"check(this.id);\">\n                    <label for=\"31\">\n                    <img class=\"image-answer\" src=\"https://www.rd.com/wp-content/uploads/2017/07/01-Foods-Queen-Elizabeth-II-Would-Never-Ever-Eat-shutterstock_580936888-1024x683.jpg\"></label></div></th>\n                <th><div class=\"picture-container\">\n                    <input type=\"checkbox\" class=\"image-checkbox\" id=\"32\" onclick=\"check(this.id);\">\n                    <label for=\"32\">\n                    <img class=\"image-answer\" src=\"https://www.rd.com/wp-content/uploads/2018/01/09_Fast_Foods-You-Should-Never-Eat-if-You-Want-to-Live-a-Long-Life_583485004_Nitr-760x506.jpg\"></label></div></th>\n            </tr>\n            <tr>\n                <th><div class=\"picture-container\">\n                    <input type=\"checkbox\" class=\"image-checkbox\" id=\"33\" onclick=\"check(this.id);\">\n                    <label for=\"33\">\n                    <img class=\"image-answer\" src=\"https://del.h-cdn.co/assets/16/32/980x490/landscape-1470763907-jack-box-tacos.jpg\"></label></div></th>\n                <th><div class=\"picture-container\">\n                    <input type=\"checkbox\" class=\"image-checkbox\" id=\"34\" onclick=\"check(this.id);\">\n                    <label for=\"34\">\n                    <img class=\"image-answer\" src=\"http://pezeshkin.ir/wp-content/uploads/2017/06/iran-watermelon.jpg\"></label></div></th>\n            </tr>\n            <tr>\n             <th><div class=\"picture-container\">\n                    <input type=\"checkbox\" class=\"image-checkbox\" id=\"35\" onclick=\"check(this.id);\">\n                    <label for=\"35\">\n                    <img class=\"image-answer\" src=\"https://www.shape.com/sites/shape.com/files/styles/story_detail/public/story/macaroons_0.jpg?itok=vg0gE31s\"></label></div></th>   \n             <th><div class=\"picture-container\">\n                    <input type=\"checkbox\" class=\"image-checkbox\" id=\"36\" onclick=\"check(this.id);\">\n                    <label for=\"36\">\n                    <img class=\"image-answer\" src=\"http://d2gxybgqxxp09o.cloudfront.net/wp-content/uploads/2015/09/pancakes.jpg\"></label></div></th>\n            </tr>\n    </tbody>\n    </table>\n</div>\n</form>\n\n<div class=\"header-container\">\n    <h1 class=\"header-text\">Where Are You Happiest?</h1>\n</div>\n\n<form>\n<div class=\"picture-question-container\">\n    <table style=\"width: 40%\">\n        <tbody>\n            <tr>\n                <th><div class=\"picture-container\">\n                    <input type=\"checkbox\" class=\"image-checkbox\" id=\"41\" onclick=\"check(this.id);\">\n                    <label for=\"41\">\n                    <div class=\"text-container\"><h1 class=\"text-answer\">Beach</h1></div></label></div></th>\n                <th><div class=\"picture-container\">\n                    <input type=\"checkbox\" class=\"image-checkbox\" id=\"42\" onclick=\"check(this.id);\">\n                    <label for=\"42\">\n                    <div class=\"text-container\"><h1 class=\"text-answer\">City</h1></div></label></div></th>\n            </tr>\n            <tr>\n                <th><div class=\"picture-container\">\n                    <input type=\"checkbox\" class=\"image-checkbox\" id=\"43\" onclick=\"check(this.id);\">\n                    <label for=\"43\">\n                        <div class=\"text-container\"><h1 class=\"text-answer\">Your Bed</h1></div></label></div></th>\n                <th><div class=\"picture-container\">\n                    <input type=\"checkbox\" class=\"image-checkbox\" id=\"44\" onclick=\"check(this.id);\">\n                    <label for=\"44\">\n                    <div class=\"text-container\"><h1 class=\"text-answer\">Forest</h1></div></label></div></th>\n            </tr>\n            <tr>\n                <th><div class=\"picture-container\">\n                    <input type=\"checkbox\" class=\"image-checkbox\" id=\"45\" onclick=\"check(this.id);\">\n                    <label for=\"45\">\n                        <div class=\"text-container\"><h1 class=\"text-answer\">Pool</h1></div></label></div></th>\n                <th><div class=\"picture-container\">\n                    <input type=\"checkbox\" class=\"image-checkbox\" id=\"46\" onclick=\"check(this.id);\">\n                    <label for=\"46\">\n                    <div class=\"text-container\"><h1 class=\"text-answer\">At a Mall</h1></div></label></div></th>\n            </tr>\n    </tbody>\n    </table>\n</div>\n</form>\n\n<div class=\"header-container\">\n    <h1 class=\"header-text\">How Do You Spend a Friday Night?</h1>\n</div>\n\n<form>\n<div class=\"picture-question-container\">\n    <table style=\"width: 40%\">\n        <tbody>\n            <tr>\n                <th><div class=\"picture-container\">\n                    <input type=\"checkbox\" class=\"image-checkbox\" id
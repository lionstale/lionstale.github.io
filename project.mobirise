            "<script>\n    function check(id) {\n        document.getElementById(id).checked = true;\n        document.getElementById(id).nextElementSibling.style.opacity = 1;\n        var i = 1;\n        while (document.getElementById(Math.floor(id/10) + \"\" + i) != null) {\n            if (i != id % 10) {\n                document.getElementById(Math.floor(id/10) + \"\" + i).checked = false;\n                document.getElementById(Math.floor(id/10) + \"\" + i).nextElementSibling.style.opacity = 0.7;\n        \t}\n            i++;\n        }\n        var question = 1;\n        var allAnswered = true;\n        while (document.getElementById(question + \"\" + 1) != null && allAnswered) {\n            var index = 1;\n            var checked = false;\n            while (document.getElementById(question + \"\" + index) != null) {\n                checked = checked || document.getElementById(question + \"\" + index).checked;\n                index++;\n            }\n            allAnswered = allAnswered && checked;\n            question++;\n        }\n        if (allAnswered) {\n            complete();\n        }\n    }\n</script>",
            "<script>\n    function Result(title, description, answers) {\n        this.title = title;\n        this.description = description;\n        this.count = 0;\n        this.answers = answers;\n    }\n    \n    function complete() {\n        var results = [\n            new Result(\"Sushi Shop\", \"Sushi shop is classy, just like you.\", [11, 13, 21, 26, 36, 42, 43, 44, 51, 56]),\n            new Result(\"Thai Cafe\", \"You are aggressively mediocre, just like everything about this restaurant.\", [15, 23, 25, 31, 32, 45, 46, 51, 55]),\n            new Result(\"Makisu\", \"You are unknown to many, but you're still there.\", [16, 21, 22, 35, 34, 44, 45, 46, 54]),\n            new Result(\"Marcel's Burger Bar\", \"You're high maintenance, but worth it.\", [11, 12, 21, 23, 34, 35, 36, 41, 46, 53]),\n            new Result(\"Oscar's\", \"You are full of wonderful qualities, but have a hard time letting people in.\", [14, 15, 23, 24, 31, 32, 44, 45, 46, 52, 54]),\n            new Result(\"Zigz\", \"You're probably vegan or wish you were.\", [12, 15, 22, 31, 33, 41, 52, 56]),\n            new Result(\"Chez Ali\", \"You like to keep it casual and might smell vaguely of old Mediterranean food.\", [16, 23, 32, 42, 45, 52]),\n            new Result(\"Exki\", \"You shine so bright that you tend to take over any room you go in to.\", [14, 24, 34, 42, 43, 55]),\n            new Result(\"Pizza Hut\", \"You're friendly and reliable, which is why people like you do much.\", [13, 23, 26, 31, 44, 54, 56]),\n            new Result(\"Anchos\", \"You tend to come off a little strong, but when people get to know you, they love you for who you really are.\", [11, 14, 25, 34, 36, 42, 43, 56])\n        ];\n        var question = 1;\n        while (document.getElementById(question + \"\" + 1) != null) {\n            var index = 1;\n            while (document.getElementById(question + \"\" + index) != null) {\n                for (var i = 0; i < results.length; i++) {\n                    document.getElementById(question + \"\" + index).className += ' no-hover';\n                    document.getElementById(question + \"\" + index).removeAttribute('onclick');\n                    if (document.getElementById(question + \"\" + index).checked && results[i].answers.includes(parseInt(question + \"\" + index))) {\n                        console.log(\"called\");\n                        results[i].count++;\n                    }\n                }\n                index++;\n            }\n            question++;\n        }\n        results.sort(function(a, b){return b.count-a.count});\n        var maxes = [];\n        var i = 0;\n        while (results[i].count == results[0].count) {\n            maxes.push(results[i]);\n            i++;\n        }\n        var max = maxes[Math.floor(Math.random() * maxes.length)];\n        document.getElementById('title-text').innerHTML = max.title;\n        document.getElementById('desc-text').innerHTML = max.description;\n        document.getElementById('answer-container').removeAttribute('hidden');\n        window.scrollTo(0,document.body.scrollHeight);\n    }\n</script>"

            "<script>\n    function Result(title, description, answers) {\n        this.title = title;\n        this.description = description;\n        this.count = 0;\n        this.answers = answers;\n    }\n    \n    function complete() {\n        var results = [\n            new Result(\"Mr. Jones\", \"Like Mr. Jones, you may be soft-spoken, but have a great sense of humor and very much enjoy doing what you love.\", [11, 13, 21, 26, 34, 42, 43, 44, 51, 55]),\n            new Result(\"Dr. Doyle\", \"Like Dr. Doyle, you have a bright personality which is why people enjoy being around you.\", [15, 23, 25, 31, 32, 45, 46, 51, 55]),\n            new Result(\"Madame Lariviere\", \"Like Madame Lariviere, you get super excited by doing what you love, sometimes even to the point of jumping up and down.\", [16, 22, 35, 36, 44, 45, 46, 54]),\n            new Result(\"Mr. Prowse\", \"Like Mr. Prowse, your sarcasm and dad jokes have no end.\", [11, 12, 21, 23, 34, 35, 36, 41, 46, 53]),\n            new Result(\"Mr. Graham\", \"Like Mr. Graham, your personality can fill any room.\", [14, 15, 23, 24, 31, 32, 44, 45, 46, 52, 54]),\n            new Result(\"Mr. van Rijn\", \"Like Mr. van Rijn, you take everything in life in a humourous way.\", [12, 15, 22, 31, 33, 41, 52, 56]),\n            new Result(\"Coach D\", \"Like Coach D, you are very calm while still being very fun to be around and people love you for it.\", [16, 23, 32, 42, 45, 52]),\n            new Result(\"Mr. Mitchell\", \"Like Mr. Mitchell, you are very passionate and tell others what you really think.\", [14, 24, 34, 42, 43, 56]),\n            new Result(\"Dr. Castillo\", \"Like Castillo, you give good advice and your friends appreciate you for it.\", [13, 23, 26, 31, 44, 54, 56]),\n            new Result(\"Dr. Carlin\", \"Like Dr. Carlin, you have unmatched knowledge of obscure scottish punk bands.\", [11, 14, 25, 34, 36, 42, 43, 56])\n        ];\n        var question = 1;\n        while (document.getElementById(question + \"\" + 1) != null) {\n            var index = 1;\n            while (document.getElementById(question + \"\" + index) != null) {\n                for (var i = 0; i < results.length; i++) {\n                    document.getElementById(question + \"\" + index).className += ' no-hover';\n                    document.getElementById(question + \"\" + index).removeAttribute('onclick');\n                    if (document.getElementById(question + \"\" + index).checked && results[i].answers.includes(parseInt(question + \"\" + index))) {\n                        console.log(\"called\");\n                        results[i].count++;\n                    }\n                }\n                index++;\n            }\n            question++;\n        }\n        results.sort(function(a, b){return b.count-a.count});\n        var maxes = [];\n        var i = 0;\n        while (results[i].count == results[0].count) {\n            maxes.push(results[i]);\n            i++;\n        }\n        var max = maxes[Math.floor(Math.random() * maxes.length)];\n        document.getElementById('title-text').innerHTML = max.title;\n        document.getElementById('desc-text').innerHTML = max.description;\n        document.getElementById('answer-container').removeAttribute('hidden');\n        window.scrollTo(0,document.body.scrollHeight);\n    }\n</script>"
